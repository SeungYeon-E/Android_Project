<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Activity.MyPage_cart_m">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마이페이지" />

    <Button
        android:id="@+id/button_cart_move"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="장바구니 n건" />

    <Button
        android:id="@+id/button_payment_move"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="결재내역" />

    <TextView
        android:id="@+id/imageView_userImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="imageView_userImage" />
    <!--    <ImageView-->
    <!--        android:id="@+id/imageView_userImage"-->
    <!--        android:layout_width="wrap_content"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        />-->
    <TextView
        android:id="@+id/textview_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="불러온ID값" />

    <Button
        android:id="@+id/button_infomation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="정보수정" />

    <TextView
        android:id="@+id/textview_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="꿀팁내역" />

    <ListView
        android:id="@+id/listView_tip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>